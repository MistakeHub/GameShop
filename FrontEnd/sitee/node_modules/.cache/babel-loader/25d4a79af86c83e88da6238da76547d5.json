f);\n      }\n      return null;\n    },\n          Register:function () {\n \n         this.$validator.validate().then(valid => {\n        if (valid) {\n          axios({\n                method: 'POST',\n                url: 'https://localhost:44303/restore',\n                params: {restoreid:this.restoreid ,password:this.Password }\n            }).then((response) => {\n             \n this.$notify({\n  group: 'foo',\n  type:'success',\n   title:\"Успешно!\",\n\n   duration:10000,\n  \n  text: \"Ваш пароль был изменён\"\n});\n             \n            }).catch(d=>{\n              if(d.response)\n         \n              this.$notify({\n  group: 'foo',\n  type:'error',\n  title: d.response.status,\n  text: d.message\n});\n  if(d.request){\n         console.log(d.request.status)\n     this.$notify({\n  group: 'foo',\n  type:'error',\n  title: 'Ошибка',\n  text:d.message\n});\n  }\n});\n        }\n      });\n             \n          }\n\n             },\n\n          \n           \n        \n          \n  }\n\n</script>"],"sourceRoot":"src/components"}]}