 \r\n            \r\n        \r\n              }\r\n          },\r\n          mounted(){\r\n          \r\n     \r\n                           axios.get('https://localhost:44303/api/Catalog/getAll').then(Response=>{ this.publications=Response.data.item1;this.countPublication=Response.data.item2;   console.log(this.publications)  } )\r\n               \r\n          },\r\n          methods:{\r\n    \r\n         \r\n        Remove:function(index){\r\n\r\n axios.delete('https://localhost:44303/api/Catalog/removePublication/'+index).then(Response=>{ this.getPublication();  console.log(this.publications)  } )\r\n          console.log(index)\r\n        },\r\n\r\n      getPublication:function(){\r\n\r\n          axios.get('https://localhost:44303/api/Catalog/getAll').then(Response=>{ this.publications=Response.data.item1;this.countPublication=Response.data.item2;   console.log(this.publications)  } )\r\n      }\r\n          \r\n   \r\n          }\r\n          \r\n           \r\n        \r\n          \r\n  }\r\n\r\n</script>"],"sourceRoot":"src/components/AdminComponents"}]}