e\" :current-page=\"currentpage\" :fields=\"fileds\">    \n       <template #cell(actions)=\"row\" >\n      \n       <b-button variant=\"danger\" size=\"sm\" @click=\"Remove(row.item.id)\" class=\"mr-1\">\n         Удаление\n        </b-button>\n\n        \n        \n        <b-button  size=\"sm\" bv-modal-example2 @click=\"ShowEditModal(row.item.id, row.item.titleofplatform)\" class=\"mr-1\"> Редактирование</b-button>\n        \n      \n      \n      </template>\n  \n   \n      </b-table>\n\n\n\n    \n      <b-pagination\n      v-model=\"currentpage\"\n      :total-rows=\"countPublication\"\n      :per-page=\"pagesize\"\n      aria-controls=\"publication-table\"\n    >\n    \n    \n \n    </b-pagination>\n  \n\n<div>\n\n<b-button v-if=\"isHaveModal\" v-b-modal.modal-1 variant=\"success\"   @click=\"$bvModal.show('bv-modal-example')\"  >Добавить</b-button>\n<b-button class=\"text-white bg-secondary\" @click=\"SaveToJson()\">Сохранить в Json</b-button>\n<b-button class=\"text-white bg-secondary\" @click=\"LoadFromJson()\" >Загрузить с JSON</b-button>\n<b-button @click=\"RemoveAll()\" variant=\"danger\">Удалить всё</b-button>\n  <b-modal id=\"bv-modal-example\" hide-footer>\n    <template #modal-title>\n    <h1>Добавление нового элемента</h1>\n    </template>\n    \n    <div class=\"d-block text-center\">\n    <label>название</label>\n      <b-input v-model=\"title\"></b-input>\n    </div>\n    <b-button class=\"mt-3\" block @click=\"Add()\">Подтвердить </b-button>\n  </b-modal>\n</div>\n\n<div>\n     <notifications group=\"foo\" />\n\n  <b-modal ref=\"bv-modal-example2\" hide-footer>\n    <template #modal-title>\n    <h1>Редактирование</h1>\n    </template>\n    \n    <div class=\"d-block text-center\">\n      <label>название</label>\n      <b-input v-model=\"title\"></b-input>\n    </div>\n    <b-button class=\"mt-3\" block @click=\"EditElement()\">Подтвердить </b-button>\n  </b-modal>\n</div>\n<notifications group=\"foo\"></notifications>\n   <router-view></router-view>\n  \n</div>\n</template>\n\n\n\n\n<script>\n\nimport axios from 'axios'\nimport store from '../../../store/index'\n  export default{\n    \n  props: {\n  },\n     \n          name: 'PlatformTableComponent',\n          data(){\n              return{\n               fileds:[],\n            \n             publications:[],\n            \n             \n                 currentpage:1,\n               countPublication:9,\n               pagesize:3,\n               Type:\"\",\n               title:\"\",\n               isHaveModal:false,\n               isModal:false,\n               AddRef:\"\",\n              Index:0,\n              \n              \n            \n        \n              }\n          },\n             computed:\n          {\n           updateIsAdmin(){return this.$store.state.isAdmin}\n\n          },\n          mounted(){\n             \n               console.log( this.$hostname)\n          \n                            if(this.$route.meta.platform)\n                           axios.get('https://localhost:44303/api/Platform/getAll', {headers:{\n                    'set-cookie':document.cookie}}).then(Response=>\n                           { this.publications=Response.data.item1;this.countPublication=Response.data.item2; this.isHaveModal=true;this.Type=\"Platform\";   console.log(this.publications)  ;this.fileds=['id', {key:'titleofplatform', label:\"Название\"}, { key: 'actions', label: 'Actions' }]} ).catch(d=>{\n              if(d.response)\n         \n              this.$notify({\n  group: 'foo',\n  type:'error',\n  title: d.response.status,\n  text: d.message\n});\n  if(d.request){\n         console.log(d.request.status)\n     this.$notify({\n  group: 'foo',\n  type:'error',\n  title: 'Ошибка',\n  text:d.message\n});\n  }\n});\n\n\n                          \n          },\n          methods:{\n\n              RemoveAll:function(){\n        this.$bvModal.msgBoxConfirm('Удалить все записи?', {\n          title: 'Пожалуйста,подтвердите',\n        