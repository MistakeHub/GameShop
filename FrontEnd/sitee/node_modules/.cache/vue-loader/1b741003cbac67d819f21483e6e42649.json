l_ofthe_week_button\"><a href=\"/catalog\">каталог</a></div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n<!-- Best Sellers -->\n\n<!-- Benefit -->\n\n<!-- Footer -->\n</div>\n   \n    \n</template>\n\n    \n\n\n<script>\n\nimport axios from 'axios'\n  export default{\n  props: {\n  },\n     \n          name: 'HelloWorld',\n          data(){\n              return{\n                 slide: 0,\n        sliding: null,\n              games:[]\n              }\n          },\n          mounted(){\n\n           axios.get('https://localhost:44303/api/Home',{headers:{\n                    \"Accept\": \"application/json\",\n                    \"Authorization\": \"Bearer \" + localStorage.getItem(\"user\") ,\n                    'set-cookie':document.cookie // передача токена в заголовке\n                }}).then(Response =>{this.games=Response.data;       console.log(this.games[0].images[0]); });\n                console.log(this.games);\n          },\n          methods: {\n              setMessage: function(event){\n                  this.message = event.target.value;\n              },\n     onSlideStart(slide) {\n        this.sliding = true\n      },\n      onSlideEnd(slide) {\n        this.sliding = false\n      },\n      \n\n      \n   \n          }\n    \n  }\n\n</script>\n\n"]}]}